package variaveis;

public class PontoFlutuante {
	public static void main(String[] args) {
		double hora = 15.30;
		System.out.println("Agora s√£o exatamente: " + hora + " horas");
		
		double conta = 3.4 / 2;
		
		System.out.println(conta);
		
		int conta2 = 3/3;
		System.out.println(conta2);
		
		double conta3 = 3.0/2;
		System.out.println(conta3);
		
		int idade = 30;
		System.out.println("Rayane Silva tem:" + idade + " anos");
	}
	

}
